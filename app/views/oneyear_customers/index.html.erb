<%- model_class = OneyearCustomer -%>
<div align="center">
<a href="http://localhost:3000/instant_customers"><button>One Time customers</button></a>
<a href="http://localhost:3000/twoyear_customers"><button>Two Year customers</button></a>
</div>

<div align="right">
<%= form_tag :controller => 'login1', 
             :action => 'logout' do %>
<%= submit_tag 'Logout', :class => 'ctrl' %>
<% end %>
</div>
<h4><center>These customers have applied for one year service.</center></h4>
<h5><center>These Customers have given $199</center></h5>
<br><br>
<form class="form-search">

    <%= text_field_tag :emp_no, params[:emp_no] %>

    <button type="submit" class="btn">Search</button>
    </form>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:emp_no) %></th>
      <th><%= model_class.human_attribute_name(:name) %></th>
      <th><%= model_class.human_attribute_name(:e_mail) %></th>
      <th><%= model_class.human_attribute_name(:phone) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
     
    </tr>
  </thead>
  <tbody>
    <% @oneyear_customers.each do |oneyear_customer| %>
      <tr>
        <td><%=  oneyear_customer.emp_no %></td>
        <td><%=  oneyear_customer.name %></td>
        <td><%=  oneyear_customer.e_mail %></td>
        <td><%=  oneyear_customer.phone %></td>
        <td><%=l oneyear_customer.created_at %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>

